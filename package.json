{
  "name": "certcity",
  "description": "",
  "version": "",
  "private": true,
  "scripts": {
    "test": "env NODE_ENV=test mocha tests/unit-tests --recursive",
    "cover": "env NODE_ENV=test istanbul cover _mocha tests/unit-tests --recursive",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "start-server": "env NODE_ENV=production forever -o ./app/.blueprint/logs/output.log -e ./app/.blueprint/logs/error.log start ./app/index.js",
    "stop-server": "env NODE_ENV=production forever stop app/index.js",
    "install-service": "sudo -s forever-service install certcity_d --script ./app/index.js",
    "uninstall-service": "sudo -s forever-service delete certcity_d"
  },
  "dependencies": {
    "@onehilltech/blueprint": "^4.21.0",
    "@onehilltech/blueprint-cli": "^4.21.0",
    "aws-sdk": "^2.683.0",
    "pug": "^2.0.4",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@onehilltech/blueprint-testing": "^4.21.0"
  },
  "license": "",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "keywords": [
    "blueprint"
  ]
}
